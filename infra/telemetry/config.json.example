{
  "host": "0.0.0.0",
  "port": 443,
  "log_level": "info",
  "json_log_enable": true,
  "namespace": "telemetry",
  "reliable_ack": true,
  "transmit_decoded_records": true,
  "kafka": {
    "bootstrap.servers": "kafka:9092",
    "queue.buffering.max.messages": 1000000
  },
  "records": {
    "alerts": [
      "logger"
    ],
    "errors": [
      "logger"
    ],
    "V": [
      "kafka"
    ]
  },
  "tls": {
    "server_cert": "/etc/letsencrypt/live/fleet-telemetry.ticketskipper.com/fullchain.pem",
    "server_key": "/etc/letsencrypt/live/fleet-telemetry.ticketskipper.com/privkey.pem"
  }
}

